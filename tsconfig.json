{
  "compilerOptions": {
    "outDir": "dist",
    "noEmit": true,
    "incremental": false,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictNullChecks": true, /* viem */
    "types": ["node"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
